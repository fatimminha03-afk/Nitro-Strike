n=int(input("Enter the number:"))
a,b=0,1
count=0
if n<=0:
    print("Please enter a positive integer:")
elif n==0:
    print("Fibonacci sequence upto:")
    print(a)
else:
    print("Fibonacci Series:")
    while count < n:
        print(a)
        nth = a+b
        a=b
        b=nth
        count+=1
